class Solution {
    public int maxProfit(int[] prices) {
        int profit = 0;
        int minp = prices[0];
        for(int i = 1;i<prices.length;i++){
          int cost = prices[i]-minp;
            profit = Math.max(profit,cost);
            minp = Math.min(minp, prices[i]);
        }
        
      return profit;  
    }
}
